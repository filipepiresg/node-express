{
  "name": "node_express",
  "version": "1.0.0",
  "description": "API de exemplo para testes",
  "main": "src/server.ts",
  "scripts": {
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "swagger": "tsoa spec",
    "build": "tsc",
    "start": "node build/server.js",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "pretty": "prettier --write",
    "commit": "npx git-cz",
    "lint": "eslint --ext .js,.ts ./src",
    "clean": "sequelize db:drop; sequelize db:create; sequelize db:migrate; sequelize db:seed:all"
  },
  "author": {
    "email": "filipepiresg@gmail.com",
    "name": "Filipe Pires"
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/integrations": "^6.17.7",
    "@sentry/node": "^6.17.7",
    "@sentry/tracing": "^6.17.7",
    "@types/express": "^4.17.13",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-actuator": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.15.0",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express-actuator": "^1.8.0",
    "@types/faker": "5.5.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.14",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config": "^0.2.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.2.0",
    "faker": "5.5.3",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint --fix"
    ]
  }
}
