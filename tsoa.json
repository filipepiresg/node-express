{
  "entryFile": "src/server.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "host": "localhost:3000",
    "schemes": ["http"],
    "basePath": "api/v1",
    "securityDefinitions": {
      "x-access-token": {
        "type": "apiKey",
        "name": "x-access-token",
        "in": "header"
      }
    },
    "outputDirectory": "public",
    "specVersion": 3
  },
  "routes": {
    "middleware": "express",
    "authenticationModule": "./src/middlewares/auth.ts",
    "routesDir": "public"
  }
}